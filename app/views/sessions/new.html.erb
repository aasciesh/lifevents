<div class="container">
<h1>Sign In</h1>
<%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
  </br>
      <%= f.submit "Sign in", class: "yt-uix-button-primary yt-uix-button" %>
    <% end %>

</div>